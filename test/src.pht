(progn
    (import java [[types [java.util.function.Supplier]]])

    (cls TestCaller [^Object] (@static
        (defn call ^Object [[lambda ^Supplier]]
            (#get lambda))))

    (cls TestProvider [^Object]
        (ctor []
            (ccall))
        (defn foo ^Object []
            "Foo!")
        (defn call ^Object []
            (#call ^TestCaller (rfn . . foo))))

    (app-fn
        (println (#call (new ^TestProvider)))))
(app
    (import java
        (types java.lang.System))
    (defn foo ^void [[arg ^dynamic]]
        (println (#toString arg)))
    (app-fn
        (def [
            [i 0]
            [start-time (#nanoTime ^System)]])
        (cycle (< i 1000)
            (#foo . "Hi!")
            (++ i))
        (println (- (#nanoTime ^System) start-time))))


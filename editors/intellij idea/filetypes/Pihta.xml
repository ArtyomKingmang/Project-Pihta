<filetype binary="false" description="Pihta files" name="Pihta">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="comment" />
      <option name="HEX_PREFIX" value="0x" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="def-macro;app-fn;app;ccall;class-of;println" ignore_case="false" />
    <keywords2 keywords="abstract;cls;cond;use-ctx" />
    <keywords3 keywords="array-of-type;array-of;array-size;array-type;break;catch;throw " />
    <keywords4 keywords="include;add;aget;alias-type;and;as-gens;as;aset;bget;body " />
  </highlighting>
  <extensionMap>
    <mapping ext="pht" />
  </extensionMap>
</filetype>
